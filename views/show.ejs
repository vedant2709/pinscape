<% include ./partials/header.ejs%>
<div class="w-full min-h-screen bg-zinc-800 px-10 py-8 text-white">
  <div class="cards flex flex-wrap gap-10 rounded-lg">
    <% user.post.forEach(function(elem){ %>

      <div class="w-32 overflow-hidden ">
        <div class="card w-32 h-40 bg-sky-200 rounded-lg">
          <img src="/images/uploads/<%= elem.image%>" class="w-full h-full object-cover rounded-lg" alt="" />
        </div>
        <h5 class="mt-2 text-sm"><%= elem.title%></h5>
      </div>
    <% })%>
  </div>
</div>
<% include ./partials/footer.ejs%>
